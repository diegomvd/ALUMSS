/*
Model Plugin for OpenMole exploration
*/


// Declaration of the model parameters
val T = Val[Double]
val L = Val[Double]
val a0 = Val[Double]
val d0 = Val[Double]
val nF = Val[Double]
val a = Val[Double]
val sAT = Val[Double]
val wS = Val[Double]
val z = Val[Double]
val dES = Val[Double]
val y0 = Val[Double]
val y1 = Val[Double]
val sFL = Val[Double]
val sR = Val[Double]
val sD = Val[Double]
val dtSave = Val[Double]
// Seed for the RNG
val mySeed = Val[Int]
// Output file for the model's simulations
val output = Val[File]

val modelPluginLocal =
  SystemExecTask(
    command = Seq(
      "./alumss-exec ${T} ${L} ${a0} ${d0} ${nF} ${a} ${sAT} ${wS} ${z} ${dES} ${y0} ${y1} ${sFL} ${sR} ${sD} ${dtSave} ${mySeed}"
    ),
  )set(
    (inputs, outputs) += (T,L,a0,d0,nF,a,sAT,wS,z,dES,y0,y1,sFL,sR,sD,dtSave,mySeed),
    outputFiles += ("DATA_AGRE",output),
    resources += "/home/karpouzi/Research/Chapter2/ALUMSS/alumss-exec",

    // default parameter and initialization values
    T:=3000.0,
    L:=40,
    a0:=0.1,
    d0:=0.0,
    nF:=1.0,
    a:=0.0,
    sAT:=1.0,
    wS:=0.0,
    z:=0.25,
    dES:=1.1,
    y0:=0.2,
    y1:=1.2,
    sFL:=0.02,
    sR:=0.2,
    sD:=0.02,
    dtSave:=1.0,
    mySeed:=21358495
  )

